{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "blazy"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/dinbror/blazy",
                    "reference": "1.8.2"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "google/charts",
                "version": "45",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "google_charts"
                },
                "dist": {
                    "url": "https://www.gstatic.com/charts/loader.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "~1.0",
        "drupal/address": "^1.7",
        "drupal/addtoany": "^2.0",
        "drupal/admin_toolbar": "^3.0",
        "drupal/advanced_email_validation": "^1.1",
        "drupal/anchor_link": "^2.0",
        "drupal/asset_injector": "^2.19",
        "drupal/back_to_top": "^3.0",
        "drupal/better_exposed_filters": "^6.0.3",
        "drupal/blazy": "^2.18",
        "drupal/block_visibility_groups": "^2.0",
        "drupal/captcha": "^2.0",
        "drupal/charts": "^5.0",
        "drupal/checklistapi": "^2.1",
        "drupal/color": "^1.0",
        "drupal/color-color": "^1.0",
        "drupal/colorbox": "^2.0",
        "drupal/colorbox_inline": "^2.0",
        "drupal/config_update": "^2.0@alpha",
        "drupal/console": "^1.0.2",
        "drupal/content_access": "^2.0",
        "drupal/core": "^9.5.9.",
        "drupal/ctools": "^4.0",
        "drupal/custom_search": "^1.0@beta",
        "drupal/date_popup": "^2.0",
        "drupal/draggableviews": "^2.0",
        "drupal/dynamic_entity_reference": "^4.0@alpha",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/embed": "^1.0",
        "drupal/encrypt": "^3.0",
        "drupal/entity": "^1.0",
        "drupal/entity_clone": "^2.0@beta",
        "drupal/entity_print": "^2.13",
        "drupal/eu_cookie_compliance": "^1.19",
        "drupal/extlink": "^1.1",
        "drupal/facebook_pixel": "^2.0@RC",
        "drupal/facets": "^3.0@beta",
        "drupal/features": "^3.8",
        "drupal/feeds": "^3.0@beta",
        "drupal/field_formatter_class": "^1.1",
        "drupal/field_group": "^3.0",
        "drupal/flag": "^4.0@beta",
        "drupal/focal_point": "^2.0",
        "drupal/fraction": "^2.1",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/geofield": "^1.54",
        "drupal/geofield_map": "^3.0",
        "drupal/geolocation": "^3.11",
        "drupal/google_analytics": "^4.0",
        "drupal/google_cse": "^4.0@alpha",
        "drupal/google_tag": "^2.0",
        "drupal/google_translator": "^2.1",
        "drupal/honeypot": "^2.1",
        "drupal/imce": "^3.0",
        "drupal/jquery_ui_accordion": "^2",
        "drupal/key": "^1.14",
        "drupal/libraries": "^4.0",
        "drupal/link_attributes": "^2.1",
        "drupal/linkit": "*",
        "drupal/mailsystem": "^4.1",
        "drupal/menu_breadcrumb": "^2.0@alpha",
        "drupal/menu_item_extras": "^3.0",
        "drupal/menu_link_attributes": "^1.3",
        "drupal/metatag": "*",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/module_filter": "^4.0",
        "drupal/paragraphs": "^1.16",
        "drupal/pathauto": "^1.3",
        "drupal/profile": "^1.0",
        "drupal/protected_pages": "^1.6",
        "drupal/quicktabs": "^3.0@alpha",
        "drupal/r4032login": "^2.1",
        "drupal/rebuild_cache_access": "^1.9",
        "drupal/recaptcha": "^3.2",
        "drupal/recurring_events": "^2.0@RC",
        "drupal/redirect": "^1.9",
        "drupal/rules": "^3.0@alpha",
        "drupal/salesforce": "^5.0",
        "drupal/salesforce_oauth": "^4.1",
        "drupal/scheduler": "^2.0",
        "drupal/schema_metatag": "*",
        "drupal/search_api": "^1.29",
        "drupal/search_api_autocomplete": "^1.7",
        "drupal/search_api_glossary": "^4.1",
        "drupal/search_api_sorts": "^1.1",
        "drupal/seckit": "^2.0",
        "drupal/simple_popup_blocks": "^3.1",
        "drupal/simple_timeline": "^2.0",
        "drupal/simplenews": "^4.0@alpha",
        "drupal/single_content_sync": "^1.4",
        "drupal/siteimprove": "^2.0",
        "drupal/smtp": "^1.2",
        "drupal/social_media_links": "^2.9",
        "drupal/superfish": "1.7",
        "drupal/token": "^1.12",
        "drupal/token_filter": "^2.0",
        "drupal/twig_tweak": "^3.1",
        "drupal/twitter_block": "^3.0@alpha",
        "drupal/typed_data": "^1.0@alpha",
        "drupal/upgrade_status": "^4.0",
        "drupal/video_embed_field": "^2.5",
        "drupal/views_accordion": "^2.0",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/views_slideshow": "^5.0",
        "drupal/webform": "^6.1",
        "drupal/webform_submission_export_import": "^6.0",
        "drupal/webform_ui": "^5.0",
        "drupal/webform_validation": "^2.0@RC",
        "drupal/webform_views": "^5.2",
        "drupal/webp": "^1.0@RC",
        "drupal/yaml_editor": "^1.1",
        "drupal/yoast_seo": "^1.8",
        "drush/drush": "*",
        "google/charts": "^45.0",
        "mikehaertl/phpwkhtmltopdf": "~2.1",
        "psr/container": "^1.0",
        "tecnickcom/tcpdf": "~6",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "zaporylie/composer-drupal-optimizations": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-drupal-scaffold-cmd": [
            "git apply -v patches/core-htaccess_redirect_https_rule_stage_mha.patch"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
        },
        "drupal/menu_item_extras": {
            "Menu Items Extras":"https://www.drupal.org/files/issues/2020-04-22/3061342-07-not-found-plugin_0.patch"
        },
        "drupal/cacheflush_cron": {
            "Cache flush Cron":"https://www.drupal.org/files/issues/2019-06-03/cacheflush_cron_fix_preset_name_-3052418-3.patch"
        },
        "drupal/entity_clone.module": {
            "Entity clone":"https://www.drupal.org/files/issues/2020-05-14/2784373_13.patch"
        },
        "drupal/draggableviews": {
            "Draggableviews": "https://www.drupal.org/files/issues/2021-10-11/not_working_with_group_by-draggableviews-2867348-48.patch"
        },
        "drupal/salesforce": {
            "Salesforce": "https://www.drupal.org/files/issues/2023-03-28/salesforce-3350924-2.patch"
        },
        "drupal/csv_serialization": {
            "csv_serialization":"https://www.drupal.org/files/issues/2022-11-03/csv_serialization-3295036-5-d10_compat.patch"
        },

        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "locations": {
                "web-root": "web"
            },
            "file-mapping": {
                "docroot/.htaccess": false,
                "docroot/web/robots.txt": false,
                "docroot/robots.txt": false,
                "docroot/.cpanel.yml": false
            },
            "excludes": [
                ".htaccess",
                "robots.txt",
                ".cpanel.yml"
            ]
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^5.4"
    }
}
