{# MHA Map render 2023-map-data #}
{# Preprocess #}
{% set year = (fields.field_year.content) %}
{% set title_year = (fields.nothing_1.content) %}
{% set map_id = (fields.field_js_map_id.content) %}
{% set map = (fields.field_js.content ) %}
{% set map_copy = (fields.field_copy.content ) %}

{# Edit #}
{{ fields.edit_node.content }}

{# Title and Year #}
<h1 class="map-title">
    {{ title_year }}
</h1>

{# Colored Lines #}
<div class="map-color-lines">
    <div class="line border-bottom">
        <span class="one"></span>
        <span class="second"></span>
        <span class="three"></span>
    </div>
</div>

<div>
    <br>
</div>

{# Map JS -- this is the actual map #}
<div class="map-js">
    {# Attach General CSS #}
    <link href="/libraries/mha-maps/2023/mhamapcolors2022.css" rel="stylesheet"
          type="text/css">

    {# Attach General JS #}
    <script src="/libraries/mha-maps/2023/ammapblue2022javascript.js" type="text/javascript"></script>

    {# Attach Node JS #}
    <script src="/libraries/mha-maps/2023/{{ map_id }}.js" type="text/javascript"></script>

    {# Attach Map #}
    {{ map }}
</div>

{# Map Bar #}
<div class="map-bar w-100 py-2">
    <img class="pull-right" alt="empty" src="/libraries/mha-maps/2023/bar.png" style="width: 250px">
</div>

{# Copy #}
<div class="map-copy py-3 px-5">
    {{ map_copy }}
</div>



